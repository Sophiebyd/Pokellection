<div class="container table-responsive-sm mt-2 col-6">
    <h1 class="text-center display-5 mt-3">Liste Boosters</h1>
    <table class="table table-hover table-striped">
        <thead class="table-primary">
            <tr>
                <th class="col-2 text-secondary">Série boosters</th>
                <th class="col-2 text-secondary text-center">Collection</th>

            </tr>
            <tbody>
            {{#each boosters}}
            {{>cartes/listbooster data=this}}
            {{/each}}
            </tbody>
        </thead>
    </table>
</div>

<div class="container table-responsive-sm mt-2 col-6">
    <h1 class="text-center display-5 mt-3">Liste Decks</h1>
    <table class="table table-hover table-striped">
        <thead class="table-primary">
            <tr>
                <th class="col-2 text-secondary">Séries decks</th>
                <th class="col-2 text-secondary text-center">Collection</th>
            </tr>
            <tbody>
            {{#each decks}}
            {{>cartes/listdeck data=this}}
            {{/each}}
            </tbody>
        </thead>
    </table>
</div>