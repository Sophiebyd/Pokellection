                            </div>




                            <div class="mb-3">
                                <label for="Password" class="form-label">Mot de passe :</label>
                                <input type="email" class="form-control rounded-5" id="exampleFormControlInput1"
                                    placeholder="name@example.com" />
                            </div>

                            <div class="mb-3">
                                <label for="confpassword" class="form-label">Confirmez mot passe :</label>
                                <input type="email" class="form-control rounded-5" id="exampleFormControlInput1"
                                    placeholder="name@example.com" />
                            </div>


                            <div class="d-grid gap-2 col-6 mx-auto position-absolute top-100 start-50 translate-middle">
                                <button class="btn btn-danger rounded-5"
                                    style="background-color: rgba(255, 0, 0, 0.813)" type="button">
                                    Soumettre
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <!-- Form Login -->
            <div class="tab-pane fade" id="pills-seconnecte" role="tabpanel" aria-labelledby="pills-seconnecte-tab"
