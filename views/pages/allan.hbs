                <div class="container-lg container-md container-sm container-xs rounded-5 position-relative pb-5 mb-5"
                    style="background-color: rgba(240, 128, 128)">
                    <div class="row-gap-3">
                        <div class="col-12 col-md-12 col-lg-12">
                            <div class="d-flex justify-content-center">
                                <h1>Formulaire de Login</h1>
                            </div>

                            <div class="mb-3">
                                <label for="Psedo/Email" class="form-label">Psedo / Email :</label>
                                <input class="form-control rounded-5" type="text" placeholder="Default input"
                                    aria-label="default input example" />
                            </div>

                            <div class="mb-3">
                                <label for="Password" class="form-label">Mot de passe :</label>
                                <input type="email" class="form-control rounded-5" id="exampleFormControlInput1"
                                    placeholder="name@example.com" />
                            </div>





                            <div class="d-grid gap-2 col-6 mx-auto position-absolute top-100 start-50 translate-middle">
                                <button class="btn btn-danger rounded-5"
                                    style="background-color: rgba(255, 0, 0, 0.813)" type="button">
                                    Soumettre
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <!-- Form Lostpassword -->
            <div class="tab-pane fade" id="pills-motdepasseoublie" role="tabpanel"
                aria-labelledby="pills-motdepasseoublie-tab" tabindex="0">

                <div class="container-lg container-md container-sm container-xs rounded-5 position-relative pb-5 mb-5"
