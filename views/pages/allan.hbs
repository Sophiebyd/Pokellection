                    <div class="row-gap-3">
                        <div class="col-12 col-md-12 col-lg-12">
                            <div class="d-flex justify-content-center">
                                <h1>Formulaire de LostPassword</h1>
                            </div>

                            <div class="mb-3">
                                <label for="Email" class="form-label">Email :</label>
                                <input class="form-control rounded-5" type="text" placeholder="Default input"
                                    aria-label="default input example" />
                            </div>



                            <div class="d-grid gap-2 col-6 mx-auto position-absolute top-100 start-50 translate-middle">
                                <button class="btn btn-danger rounded-5"
                                    style="background-color: rgba(255, 0, 0, 0.813)" type="button">
                                    Soumettre
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
