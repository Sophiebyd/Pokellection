<form class="d-flex col-lg-3 col-md-4 col-sm-6 mt-5 ml-5 ms-3 gap-3" role="search">
    <input class="form-control" type="search" placeholder="Rechercher" aria-label="Search" />
    <button class="btn btn-outline-primary " type="submit">Rechercher</button>
</form>

<!-- first table users -->
<div class="container table-responsive-sm mt-2 col-5">
    <h1 class="text-center display-5">Table users</h1>
    <table class="table table-hover table-striped">
        <thead class="table-primary">
            <tr>
                <th class="col-1 text-secondary text-center">Nom</th>
                <th class="col-1 text-secondary text-center">prénom</th>
                <th class="col-1 text-secondary text-center">Date crée</th>
                <th class="col-1 text-secondary text-center">Edité le</th>
                <th class="col-1 text-secondary text-center">Action</th>
            </tr>
        </thead>
        <tbody>
            {{#each user}}
            {{>admin/tablesusers data=this}}
            {{/each}}
        </tbody>
    </table>
</div>


<!-- second table categories -->
<div class="container table-responsive-sm mt-2 col-4">
    <h1 class="text-center display-5">Table categories</h1>
    <div class="d-flex justify-content-end">
        <a role="button" data-bs-toggle="modal" data-bs-target="#ajout_categories" class="me-4 mb-3"><i
                class="bi bi-plus-square"></i></a>
    </div>
    <table class="table table-hover table-striped">
        <thead class="table-primary">
            <tr>
                <th class="col-1 text-secondary text-center">Titre</th>
                <th class="col-1 text-secondary text-center">Date crée</th>
                <th class="col-1 text-secondary text-center">Edité le</th>
                <th class="col-1 text-secondary text-center">Action</th>
            </tr>
        <tbody>
            {{#each categorie}}
            {{>admin/tablescategories data=this}}
            {{/each}}
        </tbody>
        </thead>
    </table>
</div>

<!-- modal categories -->
<div class="modal" tabindex="-1" id="ajout_categories" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Ajouter</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">

                <!-- form categories -->
                <form action="/admin/categories" method="post" enctype="multipart/form-data">
                    <div class="row g-3">
                        <div>
                            <input type="text" class="form-control" placeholder="Nom" aria-label="name" name="post_categories">
                        </div>
                        <div class="mb-3">
                            <label for="formFile" class="form-label"></label>
                            <input class="form-control" type="file" id="form" name="ajout_image">
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="submit" class="btn btn-primary" data-bs-dismiss="modal">Ajouter</button>
                        <button type="button" class="btn btn-danger">Annuler</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>


